{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build OverlayShopTitans",
      "type": "shell",
      "command": "g++",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.exe",
        "-I",
        "${workspaceFolder}/libs",
        "-std=c++17",
        "-pthread"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    }
  ]
}
